
libzlimdbclient = cStaticLibrary + {
  root = { "$(mareDir)/src", "$(mareDir)/include" }
  includePaths = { "$(mareDir)/include" }
  files = {
    "$(mareDir)/src/*.c" = cSource
    "$(mareDir)/include/*.h"
  }
  if tool == "vcxproj" {
    libs += { "ws2_32" }
    linkFlags += { "/SUBSYSTEM:CONSOLE" }
  }
}
